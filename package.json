  },
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "----UTILS------------------------------------": "",
    "setup": "rm -rf ./node_modules && npm i && npm audit fix",
    "update": "npm-check -u",
    "----BUNDLE-----------------------------------": "",
    "build:docs": "---TBD---",
    "build": "rm -rf ./dist && babel src --out-dir dist",
    "prepare": "npm audit fix && npm test && npm run lint && npm run build",
    "----LINTING----------------------------------": "",
    "lint": "eslint src",
    "----TESTS------------------------------------": "",
    "bench": "node src/core__deep-equal/deep-equal.bench.js",
    "test": "tape -r @babel/register src/*.test.js src/**/*.test.js",
    "tdd": "nodemon --watch src --exec \"npm run test | tap-diff\""
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "7.2.0",
    "@babel/core": "7.2.0",
    "@babel/preset-env": "7.2.0",
    "@babel/register": "7.0.0",
    "@leeruniek/eslint-config": "3.0.2",
    "babel-eslint": "10.0.1",
    "benchmark": "2.1.4",
    "eslint": "5.10.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-json": "1.3.2",
    "eslint-plugin-no-inferred-method-name": "1.0.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-unicorn": "6.0.1",
    "fast-deep-equal": "2.0.1",
    "lodash": "4.17.11",
    "markdownlint-cli": "0.13.0",
    "nodemon": "1.18.8",
    "npm-check": "5.9.0",
    "prettier": "1.15.3",
    "ramda": "0.26.1",
    "tap-diff": "0.1.1",
    "tape": "4.9.1",
    "underscore": "1.9.1"
  }
}
