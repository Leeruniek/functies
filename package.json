  "author": {
    "name": "Andrei Dumitrescu",
    "email": "andrei@codemachiner.com"
  },
  "main": "dist/index.js",
  "scripts": {
    "----UTILS------------------------------------": "",
    "setup": "rm -rf ./node_modules && npm i",
    "check:update": "npm-check -u",
    "----BUNDLE-----------------------------------": "",
    "prepublish": "npm test && npm run lint && npm run build",
    "build": "babel src -d dist",
    "----LINTING----------------------------------": "",
    "lint": "eslint --fix ./src",
    "----TESTS------------------------------------": "",
    "bench": "node src/core__deep-equal/deep-equal.bench.js",
    "test": "tape src/**/*.test.js | tap-diff",
    "tdd": "nodemon --watch src --exec \"npm run test\""
  },
  "dependencies": {},
  "devDependencies": {
    "@leeruniek/eslint-config": "2.9.6",
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-1": "6.24.1",
    "benchmark": "2.1.4",
    "eslint": "4.19.1",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-no-inferred-method-name": "1.0.2",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-unicorn": "4.0.3",
    "fast-deep-equal": "2.0.1",
    "lodash": "4.17.10",
    "markdownlint-cli": "0.10.0",
    "nodemon": "1.17.5",
    "npm-check": "5.7.1",
    "ramda": "0.25.0",
    "tap-diff": "0.1.1",
    "tape": "4.9.1",
    "underscore": "1.9.1"
  },
